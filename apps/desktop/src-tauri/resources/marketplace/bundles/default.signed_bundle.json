    "policy": {
      "version": "1.1.0",
      "name": "Default",
      "description": "Default security rules for AI agent execution",
      "extends": null,
      "merge_strategy": "deep_merge",
      "guards": {
        "forbidden_path": {
          "enabled": true,
          "patterns": [
            "**/.ssh/**",
            "**/id_rsa*",
            "**/id_ed25519*",
            "**/id_ecdsa*",
            "**/.aws/**",
            "**/.env",
            "**/.env.*",
            "**/.git-credentials",
            "**/.gitconfig",
            "**/.gnupg/**",
            "**/.kube/**",
            "**/.docker/**",
            "**/.npmrc",
            "**/.password-store/**",
            "**/pass/**",
            "**/.1password/**",
            "/etc/shadow",
            "/etc/passwd",
            "/etc/sudoers"
          ],
          "exceptions": [],
          "additional_patterns": [],
          "remove_patterns": []
        },
        "egress_allowlist": {
          "enabled": true,
          "allow": [
            "*.openai.com",
            "*.anthropic.com",
            "api.github.com",
            "github.com",
            "*.githubusercontent.com",
            "*.npmjs.org",
            "registry.npmjs.org",
            "pypi.org",
            "files.pythonhosted.org",
            "crates.io",
            "static.crates.io"
          ],
          "block": [],
          "default_action": "block",
          "additional_allow": [],
          "remove_allow": [],
          "additional_block": [],
          "remove_block": []
        },
        "secret_leak": {
          "enabled": true,
          "redact": true,
          "severity_threshold": "error",
          "patterns": [
            {
              "name": "aws_access_key",
              "pattern": "AKIA[0-9A-Z]{16}",
              "severity": "critical",
              "luhn_check": false
            },
            {
              "name": "github_token",
              "pattern": "gh[ps]_[A-Za-z0-9]{36}",
              "severity": "critical",
              "luhn_check": false
            },
            {
              "name": "openai_key",
              "pattern": "sk-[A-Za-z0-9]{48}",
              "severity": "critical",
              "luhn_check": false
            },
            {
              "name": "private_key",
              "pattern": "-----BEGIN\\s+(RSA\\s+)?PRIVATE\\s+KEY-----",
              "severity": "critical",
              "luhn_check": false
            }
          ],
          "skip_paths": [
            "**/test/**",
            "**/tests/**",
            "**/*_test.*",
            "**/*.test.*"
          ]
        },
        "patch_integrity": {
          "enabled": true,
          "max_additions": 1000,
          "max_deletions": 500,
          "forbidden_patterns": [
            "(?i)disable[_\\-]?(security|auth|ssl|tls)",
            "(?i)skip[_\\-]?(verify|validation|check)",
            "(?i)rm\\s+-rf\\s+/",
            "(?i)chmod\\s+777"
          ],
          "require_balance": false,
          "max_imbalance_ratio": 10.0
        },
        "mcp_tool": {
          "enabled": true,
          "allow": [],
          "block": [
            "shell_exec",
            "run_command",
            "raw_file_write",
            "raw_file_delete"
          ],
          "require_confirmation": [
            "file_write",
            "file_delete",
            "git_push"
          ],
          "default_action": "allow",
          "max_args_size": 1048576,
          "additional_allow": [],
          "remove_allow": [],
          "additional_block": [],
          "remove_block": []
        },
        "prompt_injection": null,
        "jailbreak": null,
        "custom": []
      },
      "custom_guards": [],
      "settings": {
        "fail_fast": false,
        "verbose_logging": false,
        "session_timeout_secs": 3600
      }
    },
