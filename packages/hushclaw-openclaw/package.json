{
  "name": "@hushclaw/openclaw",
  "version": "0.1.0",
  "description": "Multi-model security enforcement plugin for OpenClaw agents",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./guards": {
      "import": "./dist/guards/index.js",
      "types": "./dist/guards/index.d.ts"
    },
    "./policy": {
      "import": "./dist/policy/index.js",
      "types": "./dist/policy/index.d.ts"
    }
  },
  "files": [
    "dist",
    "rulesets",
    "openclaw.plugin.json"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "minimatch": "^9.0.0",
    "chokidar": "^3.6.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0",
    "@vitest/coverage-v8": "^1.2.0"
  },
  "peerDependencies": {
    "openclaw": ">=0.1.0"
  },
  "peerDependenciesMeta": {
    "openclaw": {
      "optional": true
    }
  },
  "keywords": [
    "hushclaw",
    "openclaw",
    "security",
    "ai-agent",
    "policy",
    "guards"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/backbay-labs/hushclaw"
  },
  "author": "Backbay Labs",
  "engines": {
    "node": ">=18.0.0"
  }
}
