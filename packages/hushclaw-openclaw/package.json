{
  "name": "@hushclaw/openclaw",
  "version": "0.1.0",
  "description": "Hushclaw security plugin for OpenClaw",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    }
  },
  "bin": {
    "hushclaw": "./dist/cli/bin.js"
  },
  "scripts": {
    "build": "tsc",
    "e2e": "npm run build && node dist/e2e/openclaw-e2e.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "js-yaml": "^4.1.0",
    "chalk": "^5.3.0",
    "glob": "^13.0.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "openclaw": ">=0.1.0"
  },
  "peerDependenciesMeta": {
    "openclaw": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "rulesets"
  ],
  "keywords": [
    "openclaw",
    "security",
    "hushclaw",
    "ai-agent"
  ],
  "license": "MIT"
}
