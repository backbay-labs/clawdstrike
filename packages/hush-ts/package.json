{
  "name": "@clawdstrike/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for clawdstrike security verification",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "prepare": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "npm run typecheck",
    "clean": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true })\"",
    "bench": "node benchmarks/guards.bench.mjs",
    "bench:json": "OUTPUT_JSON=1 node benchmarks/guards.bench.mjs"
  },
  "keywords": [
    "clawdstrike",
    "security",
    "verification",
    "merkle",
    "ed25519",
    "receipts"
  ],
  "author": "Clawdstrike Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/backbay-labs/clawdstrike.git",
    "directory": "packages/hush-ts"
  },
  "dependencies": {
    "@clawdstrike/adapter-core": "file:../clawdstrike-adapter-core",
    "@noble/ed25519": "^3.0.0",
    "@noble/hashes": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.19.0"
  }
}
