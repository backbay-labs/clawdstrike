{
  "name": "@clawdstrike/vercel-ai",
  "version": "0.1.0",
  "description": "Minimal Clawdstrike tool wrappers for the Vercel AI SDK",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clawdstrike/adapter-core": "^0.1.0",
    "@clawdstrike/sdk": "^0.1.0"
  },
  "peerDependencies": {
    "@ai-sdk/react": ">=3.0.0",
    "ai": ">=3.0.0",
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "@ai-sdk/react": {
      "optional": true
    },
    "ai": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@ai-sdk/react": "^3.0.71",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "ai": "^6.0.69",
    "jsdom": "^28.0.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/backbay-labs/clawdstrike.git",
    "directory": "packages/clawdstrike-vercel-ai"
  },
  "homepage": "https://github.com/backbay-labs/clawdstrike",
  "bugs": {
    "url": "https://github.com/backbay-labs/clawdstrike/issues"
  },
  "keywords": [
    "clawdstrike",
    "security",
    "ai-agent",
    "vercel-ai"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  }
}
